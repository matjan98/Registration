{
  "include": ["./src/**/*"],
  "compilerOptions": {
    "outDir": "./wwwroot/",
    "sourceMap": true,
    "strict": true,
    "module": "es2015",
    "moduleResolution": "node",
    "target": "es5",
    "experimentalDecorators": true,
    "baseUrl": "src",
    "noImplicitAny": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "allowJs": false
  }
}